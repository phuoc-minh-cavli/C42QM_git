Import('env')
import os
#-------------------------------------------------------------------------------
# Convert warnings to errors in all hardware engines components
#-------------------------------------------------------------------------------

env.Append(CFLAGS = "-Werror ")

if env.CheckAlias('devcfg_img') :
	env.Append(CFLAGS = ' -Wno-unused-function ')

env.LoadSoftwareUnits()
