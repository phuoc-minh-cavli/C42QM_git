Import('env')

#-------------------------------------------------------------------------------
# Publish Private APIs
#-------------------------------------------------------------------------------
config='9205'                   #default config
if env['MSM_ID'] in ['9205']:   #add to this list as more configs are added
    config=env['MSM_ID']

env.PublishPrivateApi('QDSS_TMC_HAL', [
   "../hal/inc",
   "../../csr/hal/inc",
   "../config/" + config,
])


#-------------------------------------------------------------------------------
# Load sub scripts
#-------------------------------------------------------------------------------
env.LoadSoftwareUnits()
