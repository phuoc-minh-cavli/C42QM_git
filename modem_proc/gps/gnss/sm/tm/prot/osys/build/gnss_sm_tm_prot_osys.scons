#--------------------------------------------------------------------------------
# Import the SCons environment
#--------------------------------------------------------------------------------
Import('gps_env')
gps_osys_env = gps_env.Clone()
Export('gps_osys_env')

#--------------------------------------------------------------------------------
# Check USES flags and return if library isn't needed
#--------------------------------------------------------------------------------
if 'USES_CGPS' not in gps_osys_env:
    Return()

# ASN1 files are autogenerated using ASN1 compiler
# Disable compiler warning for always true/false checks
gps_osys_env.PrintInfo("Disable autological-compare warning")
gps_osys_env.Append(HEXAGON_WARN = ' -Wno-tautological-compare ')
    
gps_osys_env.LoadSoftwareUnits()